#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jasonlankow/Desktop/zkxyz/site-project/node_modules/sucrase/bin/node_modules:/Users/jasonlankow/Desktop/zkxyz/site-project/node_modules/sucrase/node_modules:/Users/jasonlankow/Desktop/zkxyz/site-project/node_modules:/Users/jasonlankow/Desktop/zkxyz/node_modules:/Users/jasonlankow/Desktop/node_modules:/Users/jasonlankow/node_modules:/Users/node_modules:/node_modules:/Users/jasonlankow/Desktop/zkxyz/site-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jasonlankow/Desktop/zkxyz/site-project/node_modules/sucrase/bin/node_modules:/Users/jasonlankow/Desktop/zkxyz/site-project/node_modules/sucrase/node_modules:/Users/jasonlankow/Desktop/zkxyz/site-project/node_modules:/Users/jasonlankow/Desktop/zkxyz/node_modules:/Users/jasonlankow/Desktop/node_modules:/Users/jasonlankow/node_modules:/Users/node_modules:/node_modules:/Users/jasonlankow/Desktop/zkxyz/site-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sucrase/bin/sucrase-node" "$@"
else
  exec node  "$basedir/../sucrase/bin/sucrase-node" "$@"
fi
